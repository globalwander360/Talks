keytool -importcert ^
-alias echonet_bnpparibas ^
-file "C:\softwares\echonet.bnpparibas.crt" ^
-keystore "C:\corretto-17.0 1\corretto-17.0.11\lib\security\cacerts"



keytool -list ^
-keystore "C:\corretto-17.0 1\corretto-17.0.11\lib\security\cacerts" ^
-alias echonet_bnpparibas


keytool -importcert ^
-alias artifactory_echonet ^
-file "C:\softwares\artifactory_echonet.crt" ^
-keystore "C:\corretto-17.0 1\corretto-17.0.11\lib\security\cacerts"


keytool -list ^
-keystore "C:\corretto-17.0 1\corretto-17.0.11\lib\security\cacerts" ^
-alias artifactory_echonet

keytool -delete ^
-alias echonet_bnpparibas ^
-keystore "C:\corretto-17.0 1\corretto-17.0.11\lib\security\cacerts"

set MAVEN_OPTS=-Djavax.net.ssl.trustStore=C:\softwares\echonet-truststore.jks -Djavax.net.ssl.trustStorePassword=echonet123
